<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view_controller.ControladorVistaReclamacion">
   <children>
      <HBox alignment="CENTER">
         <children>
            <Label id="lblPedidoReclamado" fx:id="lblPedidoReclamado" alignment="CENTER" contentDisplay="CENTER" text="Label" />
         </children>
      </HBox>
      <VBox spacing="5.0">
         <children>
            <Label text="Título:" />
            <TextField id="txtTitulo" fx:id="txtTitulo" />
         </children>
      </VBox>
      <VBox spacing="5.0">
         <children>
            <Label text="Descripción:" />
            <TextArea id="txtDescripcion" fx:id="txtDescripcion" prefHeight="200.0" prefWidth="483.0" />
         </children>
      </VBox>
      <HBox alignment="CENTER_RIGHT" spacing="20.0">
         <children>
            <Button id="btnVolver" fx:id="btnVolver" mnemonicParsing="false" onAction="#pressVolver" text="Volver" />
            <Button id="btnContinuar" fx:id="btnContinuar" mnemonicParsing="false" onAction="#pressContinuar" text="Continuar" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
